<% label = instance_variable_defined?(:@label) ? @label : nil %>
<% name = instance_variable_defined?(:@name) ? @name : nil %>
<% no_names = instance_variable_defined?(:@no_names) ? @no_names : nil %>
<% no_types = instance_variable_defined?(:@no_types) ? @no_types : nil %>
<% title = label ? label : YARD::Tags::Library.labels[name] %>
<%= title %>:
<%= hr(title.length) %>-

<% object.tags(name).each do |tag| %>
<% text = "" %>
<% text += format_types(tag.types) + " " unless no_types || tag.types.nil? %>
<% text += tag.name.to_s + " " unless no_names %>
<% text += "- " unless no_names && no_types %>
<% text += tag.text if tag.text && !tag.text.empty? %>
<%= indent wrap(text) %>

<% end %>